(false)\n    setClick(!click)}\n  const handleClick = () => setClick(!click);\n  return (\n    <>\n      <nav className=\"navbar\">\n        <div className=\"nav-container\">\n          <NavLink exact to=\"/\" className=\"nav-logo\">\n            Music Collab\n            <i className=\"fas fa-code\"></i>\n          </NavLink>\n\n          <ul className={click ? \"nav-menu active\" : \"nav-menu\"}>\n            <li className=\"nav-item\">\n              <NavLink\n                exact\n                to=\"/Profile\"\n                activeClassName=\"active\"\n                className=\"nav-links\"\n                onClick={handleClick}\n              >\n                Profile\n              </NavLink>\n            </li>\n            {/* <li className=\"nav-item\">\n              <NavLink\n                exact\n                to=\"/ProfileSettings\"\n                activeClassName=\"active\"\n                className=\"nav-links\"\n                onClick={handleClick}\n              >\n                Settings\n              </NavLink>\n            </li> */}\n            <li className=\"nav-item\">\n              <NavLink\n                exact\n                to=\"/Explore\"\n                activeClassName=\"active\"\n                className=\"nav-links\"\n                onClick={handleClick}\n              >\n                Explore\n              </NavLink>\n            </li>\n            <li className=\"nav-item\">\n              <NavLink\n                exact\n                to=\"/Home\"\n                activeClassName=\"active\"\n                className=\"nav-links\"\n                onClick={handleClick}\n              >\n\t               Home Page\n              </NavLink>\n            </li>\n            <li className=\"nav-item\">\n              <NavLink\n                exact\n                to=\"/\"\n                activeClassName=\"active\"\n                className=\"nav-links\"\n                onClick={logOut}\n              >\n\t